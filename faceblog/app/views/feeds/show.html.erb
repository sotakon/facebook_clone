<h1>Details</h1>
<p id="notice"><%= notice %></p>
<p>
  <strong>Image:</strong>
  <% if !@feed.image.url.nil? %>
  <%= image_tag @feed.image.url %>
  <% end %>
  <strong>message:</strong>
  <%= @feed.content %>
</p>
<% if @feed.user == current_user %>
  <%= link_to "Edit", edit_feed_path %>
<% else %>
  <a></a>
<% end %>
<%= link_to 'Back', feeds_path %>
